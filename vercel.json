{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1.js"
    }
  ],
  "env": {
    "PLAYWRIGHT_BROWSERS_PATH": "0",
    "AWS_LAMBDA_FUNCTION_MEMORY_SIZE": "1024"
  }
}
